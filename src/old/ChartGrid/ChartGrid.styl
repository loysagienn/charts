
.grid
    position: absolute
    left: 0
    top: 0
    overflow: hidden

.gridLines
    position: absolute
    left: -10px
    top: 0
    right: -10px
    bottom: 0
    z-index: 2
    // opacity: 0
    // transition: opacity 0.2s ease-out 0.5s

.gridLine
    position: absolute
    left: 0
    right: 0
    box-sizing: border-box
    padding: 5px 10px
    border-bottom: 1px solid rgba(0,0,0,0.1)
    font-size: 12px;
    color: #808080;
    user-select: none;
